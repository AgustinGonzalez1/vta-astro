---
interface Props {
	title: string;
}

const { title } = Astro.props;

import "@/style/style.css";
import BaseHead from "@/components/BaseHead.astro";
---

<!doctype html>
<html lang="en">
	<BaseHead title={title} />
	<body class="bg-dark-blue">
		<div>
			<main>
				<slot />
			</main>
		</div>
	</body>
</html>
